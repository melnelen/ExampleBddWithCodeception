{
  "name": "melnelen/examplebddwithcodeception",
  "description": "Example BDD with Codeception",
  "license": "mit",
  "require": {
    "codeception/codeception": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "scripts": {
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ],
    "check-format": [
      "vendor/bin/php-cs-fixer fix --verbose --dry-run"
    ]
  }
}
